{
    "name": "clarkwinkelmann/flarum-ext-watch-search",
    "description": "Follow search queries",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": ">=0.1.0-beta.13 <0.1.0-beta.14"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\WatchSearch\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Watch Search",
            "icon": {
                "name": "fas fa-search-plus",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        }
    }
}
